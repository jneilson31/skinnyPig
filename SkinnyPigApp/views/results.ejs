<!DOCTYPE html>
<html>

<head>
<% include partials/headrefs %>
</head>

<body>

	<% include partials/header %>


	<div class="main">
		<div class="container">
			<div class="row">
				<div class="col-sm-12">
					<div class="productBox">
						<h3 class="categoryTitle">Search Results for "
							<%=query%>"</h3>
						<div class="row">

							<% data.findItemsAdvancedResponse[0].searchResult[0].item.forEach(function(product) {
									if(product.galleryURL != "http:\/\/thumbs1.ebaystatic.com\/pict\/04040_0.jpg") {
		%>

								<div class="col-xs-6 col-md-3">
									<div class="productThumbnailCategory">
										<div class="productTitle">
											<%=product.title%>
										</div>
										<img src="<%=product.galleryURL%>" alt="..." class="center-block">
										<div class="caption">
											<div class="price">$
												<%=parseFloat(product.sellingStatus[0].currentPrice[0].__value__.toString()).toFixed(2)%>
											</div>
											<p>
												<a href="<%=product.viewItemURL%>" class="btn dealBtn-Primary" role="button" target="blank">View Deal</a>
											</p>
										</div>
									</div>
								</div>

								<%  }}) %>

						</div>
					</div>
				</div>
			</div>
		</div>

		<a href="#" id="loadMore">View More Results</a>
		<p class="totop">
			<a href="#top" id="topper">Back to top</a>
		</p>

		<% include partials/footer %>
	</div>



	<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q"
	 crossorigin="anonymous"></script>



</body>




</html>